includes:
    - phpstan-baseline.neon

parameters:
    fileExtensions:
        - php
        - phtml

    level: max

    paths:
        - index.php
        - app
        - resources/views
        - tests

    ignoreErrors:
        - '#^Call to an undefined method PHPUnit\\Framework\\MockObject\\Builder\\InvocationStubber::with\(\)\.$#'
        - '#^Cannot call method willReturn\(\) on mixed\.$#'